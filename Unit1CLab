using System;

public class Program
{
	public void Main()
	{
		string[] cars = {"1", "2", "3", "4", "5"};
		// writes the length, writes them all in order, and can put words before and after the list
		Console.WriteLine(cars.Length);
		foreach(var car in cars) {
			Console.WriteLine("I love the number " + car);
		} // what gets writen is "I love the number 1." then bwlow with the number 2, etc
		
	}
}




